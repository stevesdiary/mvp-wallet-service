{
  "name": "mvp-wallet-service",
  "version": "1.0.0",
  "description": "mvp loan wallet service",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node build/src/index.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- src/migrations/",
    "migration:create": "npm run typeorm migration:create -- src/migrations/",
    "migration:revert": "npm run typeorm migration:revert -- -d src/data-source.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/data-source.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevesdiary/mvp-wallet-service.git"
  },
  "keywords": [
    "walllet",
    "transfer",
    "loan"
  ],
