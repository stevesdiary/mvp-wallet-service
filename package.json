  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevesdiary/mvp-wallet-service/issues"
  },
  "homepage": "https://github.com/stevesdiary/mvp-wallet-service#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "jest": "^29.7.0",
    "typescript": "^5.6.3"
  }
